# Production Environment Variables
# Copy this to .env.production for optimized production builds

# Disable backend checks in production (when no backend is deployed)
VITE_BACKEND_URL=

# Optional: Supabase for user accounts (add your credentials)
VITE_SUPABASE_URL=
VITE_SUPABASE_ANON_KEY=

# Optional: Stripe for payments (add your live keys)
VITE_STRIPE_PUBLISHABLE_KEY=

# Optional: Analytics
VITE_GA_MEASUREMENT_ID=

# App configuration
VITE_APP_NAME="Img V2 Pro"
VITE_APP_DESCRIPTION="Professional image converter - Convert JPG, PNG, WEBP, SVG instantly"